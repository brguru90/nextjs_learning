{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "DEPLOY=node NODE_ENV=production next build",
    "start": "next start",
    "export": "DEPLOY=cdn next export",
    "build_ssg": "DEPLOY=cdn next build",
    "ssg": "npm-run-all --serial build_ssg export",
    "lint": "next lint",
    "build_start": "npm-run-all --serial build start",
    "cust_server": "nodemon server.js",
    "cust_server_dev": "DEPLOY=node NODE_ENV=development NEXT_SERVER=true nodemon server.js",
    "cust_server_prod": "DEPLOY=node NODE_ENV=production NEXT_SERVER=true npm-run-all --serial build cust_server"
  },
  "dependencies": {
    "dotenv": "^16.0.0",
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass": "^1.49.8",
    "swr": "^1.2.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.1.0",
    "cross-env": "^7.0.3",
    "eslint": "8.9.0",
    "eslint-config-next": "12.1.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5"
  }
}
